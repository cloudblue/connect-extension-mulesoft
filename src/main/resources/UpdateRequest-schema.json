{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "type" : "object",
  "properties" : {
    "id" : {
      "type" : "string"
    },
    "asset" : {
      "type" : "object",
      "properties" : {
        "params" : {
          "type" : "array",
          "items" : {
            "type" : "object",
            "properties" : {
              "id" : {
                "type" : "string"
              },
              "structured_value" : {
                "type" : "object"
              },
              "value" : {
                "type" : "string"
              },
              "value_error" : {
                "type" : "string"
              }
            },
            "required": ["id"]
          }
        }
      },
      "required": ["params"]
    }
  },
  "required": ["id", "asset"]
}